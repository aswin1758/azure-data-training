{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "qbdatafactory1"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorageConnection2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageConnection2'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"adls_qbstorageaccount1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_qbstorageaccount1'"
		},
		"linktoqbblobstorageaccount2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linktoqbblobstorageaccount2'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://qbstorageaccount1.dfs.core.windows.net"
		},
		"adls_qbstorageaccount1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://qbstorageaccount1.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "qbdatafactory1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorageConnection2_connectionString": {
						"value": "[parameters('AzureBlobStorageConnection2_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"adls_qbstorageaccount1_accountKey": {
						"value": "[parameters('adls_qbstorageaccount1_accountKey')]"
					},
					"linktoqbblobstorageaccount2_connectionString": {
						"value": "[parameters('linktoqbblobstorageaccount2_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"adls_qbstorageaccount1_properties_typeProperties_url": {
						"value": "[parameters('adls_qbstorageaccount1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}